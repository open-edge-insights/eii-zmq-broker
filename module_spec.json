{
    "SchemaVersion": "0.2",
    "Runtime": "docker",
    "RuntimeOptions": {
        "ContainerImage": "ia_eis_zmq_broker:$EIS_VERSION",
        "TemporaryVolumes": [
            "${EIS_INSTALL_PATH}/sockets:${SOCKET_DIR}"
        ]
    },
    "DataStore": {
        "Protocol": "DIRECT",
        "DataBuckets": [
            {
                "Name": "EIS/EISZmqBroker",
                "Permissions": "RO"
            },
            {
                "Name": "EIS/GlobalEnv",
                "Permissions": "RO"
            },
            {
                "Name": "EIS/Publickeys",
                "Permissions": "RO"
            }
        ]
    }
}
